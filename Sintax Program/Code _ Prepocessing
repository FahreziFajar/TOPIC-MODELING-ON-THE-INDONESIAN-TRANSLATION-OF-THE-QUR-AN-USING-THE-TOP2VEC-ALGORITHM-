n","    text = re.sub(r'\\d+', '', text)  # Hapus angka\n","    text = re.sub(r'[^\\x00-\\x7F]+', '', text)  # Hapus karakter non-ASCII\n","    text = text.lower()  # Case folding\n","    return text\n","\n","# 2. Normalisasi kata ulang (anak-anak → anak)\n","def normalize_reduplication(tokens):\n","    return [word.split('-')[0] if '-' in word else word for word in tokens]\n","\n","# Membersihkan kolom \"terjemahan\"\n","df['terjemahan_bersih'] = df['terjemahan'].apply(clean_text)\n","\n","# Membuat dataset baru 'translation_cleaned'\n","translation_cleaned = df[['surah', 'ayat', 'terjemahan_bersih']]\n","\n","# Menampilkan beberapa baris pertama dataset translation_cleaned\n","print(translation_cleaned.head())\n","\n","# Menyimpan dataset yang sudah dibersihkan ke file CSV baru\n","translation_cleaned.to_csv(\"quran_translation_cleaned.csv\", index=False, encoding=\"utf-8\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"-b_RpI18z9Po","executionInfo":{"status":"ok","timestamp":1749888295519,"user_tz":-420,"elapsed":198,"user":{"displayName":"Fajar Syarul Fahrezi","userId":"14363625464919180248"}},"outputId":"4caed862-56da-42d5-88de-6c86b77a66ec"},"execution_count":4,"outputs":[{"output_type":"stream","name":"stdout","text":["   surah  ayat                                  terjemahan_bersih\n","0      1     1  dengan menyebut nama allah yang maha pemurah l...\n","1      1     2          segala puji bagi allah tuhan semesta alam\n","2      1     3                   maha pemurah lagi maha penyayang\n","3      1     4                  yang menguasai di hari pembalasan\n","4      1     5  hanya engkaulah yang kami sembah dan hanya kep...\n"]}]},{"cell_type":"code","execution_count":5,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":3938,"status":"ok","timestamp":1749888316096,"user":{"displayName":"Fajar Syarul Fahrezi","userId":"14363625464919180248"},"user_tz":-420},"id":"i62UtPAqorVE","outputId":"97cedf5f-4dc7-4a9f-82d9-32c5df62a673"},"outputs":[{"output_type":"stream","name":"stderr","text":["[nltk_data] Downloading package punkt_tab to /root/nltk_data...\n","[nltk_data]   Unzipping tokenizers/punkt_tab.zip.\n"]},{"output_type":"stream","name":"stdout","text":["Tokenization selesai. File disimpan sebagai quran_translation_tokenized.csv\n"]}],"source":["# Tokenized\n","\n","import pandas as pd\n","import nltk\n","from nltk.tokenize import word_tokenize\n","\n","# Unduh resource tokenizer NLTK jika belum tersedia\n","nltk.download('punkt_tab')\n","\n","# Membaca file CSV\n","file_path = '/content/quran_translation_cleaned.csv'  # Ganti dengan path file yang sesuai\n","df = pd.read_csv(file_path)\n","\n","# Tokenisasi\n","def tokenize_text(text):\n","    tokens = word_tokenize(text)\n","    return normalize_reduplication(tokens)\n","\n","# Pastikan kolom 'terjemahan_bersih' ada dalam dataframe\n","if 'terjemahan_bersih' in df.columns:\n","    # Melakukan tokenisasi pada setiap baris dalam kolom 'terjemahan_bersih'\n","    df['terjemahan_tokenized'] = df['terjemahan_bersih'].astype(str).apply(word_tokenize)\n","\n","    # Simpan hasil tokenisasi ke file baru\n","    output_file = 'quran_translation_tokenized.csv'\n","    df.to_csv(output_file, index=False)\n","    print(f\"Tokenization selesai. File disimpan sebagai {output_file}\")\n","else:\n","    print(\"Kolom 'Isi_bersih' tidak ditemukan dalam file.\")"]},{"cell_type":"code","source":["import pandas as pd\n","import nltk\n","from nltk.corpus import stopwords\n","from nltk.tokenize import word_tokenize\n","\n","nltk.download('stopwords')\n","nltk.download('punkt')\n","\n","file_path = '/content/quran_translation_tokenized.csv'\n","df = pd.read_csv(file_path)\n","\n","# Gabungkan stopword NLTK dan tambahan\n","additional_stopwords = [\n","    'adalah', 'adapun', 'agar', 'alangkah', 'ambil', 'bahwa','barang',\n","    'barangsiapa', 'barangkali', 'bawa', 'beri', 'berapa', 'betapa',\n","    'binasakan', 'dengar', 'demi', 'hai', 'hamba', 'hendaklah', 'inilah',\n","    'ingat', 'itulah', 'jadi', 'jahannam', 'katakan', 'keluarga','kemudian', 'kembali', 'ketika',\n","    'lihat', 'maha', 'maka', 'melainkan', 'muka', 'niscaya', 'orang',\n","    'padahal', 'pastilah', 'pelihara', 'pergi', 'pasti', 'sebahagian',\n","    'sebelum', 'sebenarnya', 'sehingga', 'sekalikali', 'sembunyi',\n","    'sesudah', 'sesungguhnya', 'sungguh', 'sungguhlah', 'supaya', 'tatkala',\n","    'tahu', 'tentulah', 'tersebut', 'tiadalah', 'wahai', 'yakni', 'yaitu']\n","\n","stop_words = set(stopwords.words('indonesian')).union(additional_stopwords)\n","\n","# Fungsi untuk menghapus stopwords dari list token\n","def remove_stopwords(tokens):\n","    return [word for word in tokens if word.lower() not in stop_words]\n","\n","if 'terjemahan_tokenized' in df.columns:\n","    df['terjemahan_no_stopwords'] = df['terjemahan_tokenized'].astype(str).apply(lambda x: remove_stopwords(eval(x)))\n","    output_file = 'quran_translation_no_stopword.csv'\n","    df.to_csv(output_file, index=False)\n","    print(f\"Stopword removal selesai. File disimpan sebagai {output_file}\")\n","else:\n","    print(\"Kolom 'terjemahan_tokenized' tidak ditemukan dalam file.\")\n"],"metadata":{"id":"f8GSneTS0zzP","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1749888339950,"user_tz":-420,"elapsed":1194,"user":{"displayName":"Fajar Syarul Fahrezi","userId":"14363625464919180248"}},"outputId":"59641b80-5ae7-43be-c4e5-eae96a49419d"},"execution_count":6,"outputs":[{"output_type":"stream","name":"stderr","text":["[nltk_data] Downloading package stopwords to /root/nltk_data...\n","[nltk_data]   Unzipping corpora/stopwords.zip.\n","[nltk_data] Downloading package punkt to /root/nltk_data...\n","[nltk_data]   Unzipping tokenizers/punkt.zip.\n"]},{"output_type":"stream","name":"stdout","text":["Stopword removal selesai. File disimpan sebagai quran_translation_no_stopword.csv\n"]}]},{"cell_type":"code","source":["# lematization\n","\n","!pip install sastrawi\n","import pandas as pd\n","import nltk\n","from nltk.tokenize import word_tokenize\n","from nltk.corpus import stopwords\n","from Sastrawi.Stemmer.StemmerFactory import StemmerFactory\n","\n","# Unduh resource jika belum ada\n","nltk.download('punkt')\n","nltk.download('stopwords')\n","\n","try:\n","    # Inisialisasi stemmer\n","    factory = StemmerFactory()\n","    stemmer = factory.create_stemmer()\n","except LookupError:\n","    !pip install sastrawi\n","    factory = StemmerFactory()\n","    stemmer = factory.create_stemmer()\n","\n","# Membaca file CSV yang telah dihilangkan stopwords-nya\n","file_path = '/content/quran_translation_no_stopword.csv'\n","df = pd.read_csv(file_path)\n","\n","# Pastikan kolom 'Isi_no_stopwords' ada dalam DataFrame\n","if 'terjemahan_no_stopwords' in df.columns:\n","    # Fungsi untuk melakukan stemming pada list token\n","    def lemmatize_text(tokens):\n","        # Pastikan input berupa string yang merepresentasikan list\n","        try:\n","            token_list = eval(tokens)  # Mengubah string ke list\n","            lemmatized_tokens = [stemmer.stem(word) for word in token_list]\n","            return lemmatized_tokens\n","        except (SyntaxError, NameError, TypeError):\n","            print(f\"Error processing: {tokens}\")  # Menampilkan pesan error\n","            return []  # Mengembalikan list kosong jika terjadi error\n","\n","\n","    # Melakukan lemmatization\n","    df['terjemahan_lemmatized'] = df['terjemahan_no_stopwords'].apply(lemmatize_text)\n","\n","    # Simpan hasil lemmatization ke file baru\n","    output_file = 'quran_translation_prepocessed.csv'\n","    df.to_csv(output_file, index=False)\n","    print(f\"Lemmatization selesai. File disimpan sebagai {output_file}\")\n","else:\n","    print(\"Kolom 'Isi_no_stopwords' tidak ditemukan dalam file.\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"51nMRNVfI7D-","executionInfo":{"status":"ok